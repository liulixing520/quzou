	    var _SHOW_BTNS_ = true, _IWH_ = null, _IPT_ = null;
	    $(function () {
	        //getOrderList();
	        getInitContext();
	        window.addEventListener('orientationchange', getInitContext, false);
	
	        function showFootBar() {
	            $(window).height() >= _IWH_ ? $('.btns-fixed').slideDown(100) : $('.btns-fixed').hide();
	        }
	
	        $('#orderSearchKeyValue').click(function (event) {
	            _SHOW_BTNS_ = false;
	            $('.btns-fixed').hide();
	            window.setTimeout(function () {
	                $(window).resize(hideOrShowIt);
	            }, 500);
	            doRemove(event);
	        }).blur(function () {
	            _SHOW_BTNS_ = true;
	            window.setTimeout(function () {
	                if (_SHOW_BTNS_) {
						//$(window).unbind('resize',hideOrShowIt);
	                    showFootBar();
	                }
	            }, 50);
	        });
	
	        function hideOrShowIt() {
	            window.setTimeout(function () {
	                showFootBar();
	            }, 50);
	        }
	
	        window.setInterval(function () {
	            $('#debugWrap').append(document.getElementById('orderSearchKeyValue').style.imeMode);
	        }, 1000);
	    });
	    function getInitContext() {
	        _IWH_ = $(window).height();
	        _IPT_ = {
	            offset: $('#orderSearchKeyValue').offset(),
	            width: $('#orderSearchKeyValue').width()
	        }
	    }
	
	    function doRemove(e) {
	        if (_IPT_.offset.left + _IPT_.width - e.pageX < 60) {//点击X时清除文字
	            deletecontent();
	        }
	    }
	
	    function deletecontent() {
	        $('#orderSearchKeyValue').val('');
	        $('.search-box').removeClass('searching');
			//window.setTimeout(function () {
			//	$('#orderSearchKeyValue').focus();
			//}, 50);
	    }
	
	    function _debug(txt) {
	        $('#debug-info').append('<p>' + txt + '</p>');
	    }
	
	    </script>
	
	
