								<include-form name="FindDistributionRegion" location="component://itea/widget/iteamgr/SystemForms.xml" />
							</decorator-section>
							<decorator-section name="search-results">
								<include-form name="ListDistributionRegion" location="component://itea/widget/iteamgr/SystemForms.xml" />
							</decorator-section>
						</decorator-screen>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
