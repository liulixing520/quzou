function checkForm(){
	var result = true;
	
	result = checkStroeNo();
	
	var img1Md5 = $("#img1Md5").val();
	var img1Url = $("#img1Url").val();
	var img2Md5 = $("#img2Md5").val();
	var img2Url = $("#img2Url").val();
	
	
	if(img1Md5=="" || img1Url=="" ){
		$("#msg_img1").html("内容不能为空，请重新选择。");
		$("#msg_img1").show();
		result = false;
	}else{
		$("#msg_img1").hide();
	}
	
	if(img2Md5=="" || img2Url=="" ){
		$("#msg_img2").html("内容不能为空，请重新选择。");
		$("#msg_img2").show();
		result = false;
	}else{
		$("#msg_img2").hide();
	}
	
	return result;
}

function checkStroeNo(){
	var storeNo = $("#storeNo").val();
	storeNo =jQuery.trim(storeNo); 
	
	if(storeNo == ""){
		$("#msg_storeNo").html("内容不能为空，请重新填写。");
		$("#msg_storeNo").show();
		return false;
	}
	
	
	if(storeNo.length>9){
		$("#msg_storeNo").html("输入的数字不能超过9位。");
		$("#msg_storeNo").show();
		return false;
	}
	
	var reg = new RegExp("^([1-9][0-9]{4,8})$");
	
	if(!reg.test(storeNo)){
		$("#msg_storeNo").html("您输入的店铺号不正确，请重新输入。");
		$("#msg_storeNo").show();
		return false;
    }
	
	$("#msg_storeNo").hide();
	return true;
}

