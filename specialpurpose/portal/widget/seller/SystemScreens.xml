					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
	<screen name="FindRegion">
		<section>
			<actions>
				<property-map resource="AdminUiLabels" map-name="uiLabelMap" global="true" />
				<set field="parameters.noConditionFind" value="Y" />
				<set field="parameters.geoTypeId" value="GROUP" />
				<set field="searchAction" value="FindRegion" />
				<set field="searchForm" value="FindRegion" />
				<entity-condition list="geoList" entity-name="GeoAssocAndGeoTo">
					<condition-list>
						<condition-expr field-name="geoIdFrom" value="CHN"/>
						<condition-list combine="or">
							<condition-expr field-name="geoTypeId" value="MUNICIPALITY"/>
							<condition-expr field-name="geoTypeId" value="PROVINCE"/>
						</condition-list>
					</condition-list>
					<order-by field-name="sequenceNum"/>
				</entity-condition>
			</actions>
			<widgets>
				<decorator-screen name="product-decorator">
					<decorator-section name="body">
						<platform-specific>
							<html>
								<html-template location="component://portal/webapp/seller//system/FindRegion.ftl" />
							</html>
						</platform-specific>
					</decorator-section>
				</decorator-screen>
			</widgets>
		</section>
	</screen>
