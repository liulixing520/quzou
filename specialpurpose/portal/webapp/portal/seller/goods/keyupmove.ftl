            <script type="text/javascript">
 
	
/**
*
*上传操作初始化控件
*一次提多张图片
	
	使用说明：
		1 initsingleUploadify(); 				初始化调用
		2 validateOnSelect(event, ID, fileObj)  数据验证   
		3 saveBrandAppeal(imgmd5, imgurl, imgsize, localfilename);					上传成功后操
*   参数:
*       1 fileExtArr: 允许文件上传的格式数组(如:['jpg', 'gif'])，如果为空默认*.jpg;*.gif;*.pdf;*.rar;*.doc;*.docx;*.txt	
*/

//全局变量，用来保存后台传过来的数据
var fName;//=jQuery('#functionname').val();
var sid;//=jQuery('#supplierid').val();
var iName;//= jQuery('#imagebannername').val();


//callBackObj ={"callType":"brand/attr",..attrId:attrId,attrValId:attrValId...}
function initsingleUploadifyImg(fileExtArr, callBackObj){
	var fileExt = '*.jpg;*.gif;*.pdf;*.rar;*.doc;*.docx;*.txt';
	var fileDesc = 'choose *.jpg/*.gif/*.pdf/*.rar/*.doc/*.docx';
	if (typeof(fileExtArr) != "undefined" && fileExtArr != null && fileExtArr.length > 0) {
		fileExt = '*.' + fileExtArr.join(";*.");
		fileDesc = 'choose ' +  fileExtArr.join("/*.");
	}
	
	jQuery("#attrValFileupload").uploadify({
		'uploader'       : 'http://seller.dhgate.com/uploadjs/uploadalbums.swf',
		'script'		 : 'http://upload.dhgate.com/fileuploadservlet',
		'cancelImg'      : 'http://image.dhgate.com/2008/web20/seller/uploadjs/cancel.png',
		'folder'         : 'uploaddify',
		'scriptAccess'   : 'always', 								// Set to "always" to allow script access across domains
		'method'         : 'GET',
		'queueID'        : 'fileQueueImg',
		'auto'           : false,									//是否选取文件后自动上传    
		'multi'          : false,									//是否支持多文件上传
		'simUploadLimit' : 1,										// 允许同时上传的个数 默认值
		'sizeLimit'		 : 20480000,           						//控制上传文件的大小，单位byte
		'wmode'          : 'transparent',
		'width'			 : 120,
		'height'		 : 26,
		'fileExt'		 : fileExt,							// 设置可以选择的文件的类型，格式如：'*.doc;*.pdf;*.rar' 。
		'fileDesc'		 : fileDesc, // 设置文件浏览对话框中的文件类型下拉框的显示文本。
		'onInit'         : function() { 
								fName = jQuery('#functionname_avim').val();
								sid = jQuery('#supplierid').val();
								iName = jQuery('#imagebannername_avim').val();
		},
		'onError' 		 : function(event, ID, fileObj, errorObj){
								jQuery("#operate_message").html("Upload unsuccessful. Please try again.").show();
	    },
		'onSelect' 		 : function(event, ID, fileObj){
								if(validateOnSelect(event, ID, fileObj , callBackObj)){
									var functionname = fName;
									var supplierid = sid;
									var imagebannername = iName;
								
									//设置上传参数
									jQuery('#attrValFileupload').uploadifySettings('scriptData',{'functionname':functionname,'supplierid':supplierid,'imagebannername':imagebannername});
									//上传
									jQuery('#attrValFileupload').uploadifyUpload();
								}
						   },
        'onComplete'     : function(event,queueId,fileObj,response,data) { //成功后处理图片显示
        						if(response != ""){	 
									var s_value=window.eval("("+response+")");
									if(s_value.result=='1' || s_value.result=='2'){
										var imgmd5=s_value.l_imgmd5;
										var imgurl=s_value.l_imgurl;
										var imgsize=s_value.l_imgsize;
										var localfilename=decodeURIComponent(s_value.l_localfilename);
										saveBrandAppeal(imgmd5, imgurl, imgsize, localfilename, callBackObj);
										jQuery("#operate_message").html("上传成功").show();
									} else if (s_value.result=='0') {
										// alert(s_value.errorinfo.msg);
										jQuery("#operate_message").html(msg_upload_html1).show();
									}
								}else{
										jQuery("#operate_message").html(msg_upload_html2).show();
								}
    					   },
    	'onAllComplete'		: function(event,queueId,fileObj,response,data) { //成功后处理图片显示
    					   }
    	});
};


function delUploadInfo(){
	jQuery.modal.close();
    jQuery('#popcon').html("");		
}

</script>
