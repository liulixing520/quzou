define('mm-ae-pitem-bestsellingtop10', function(require, exports) {

	// ģʼ
	exports.init = function(box, module) {
		var template = $(".X-template-top10").html();

		var letterNums = 55;
		if ($("#bestselling-top10 .top10-item").length > 0) {
			// ̬
			$("#bestselling-top10").removeClass("loading");
			var itemHeight = $("#bestselling-top10 .top10-item").outerHeight(true);
			$("#bestselling-top10").height(itemHeight * 2+1);
		} else if (!!template) {
			// 첽ȡ
			var userLocale = require("");
			var mustache = require('');
			var cookieInfo = require("");
			var memberSeq = cookieInfo.getIsLoggedIn() ? cookieInfo.getMemberSeq() : "";
			var dtd = userLocale.getCountryCode();
			var ajaxUrl="";
			// ҳ
			var pageLang = $(".mm-ae-pitem-bestsellingtop10").attr("data-lang")||"en";

			if(pageLang==="ru"){
				ajaxUrl+="RU&language=ru";
