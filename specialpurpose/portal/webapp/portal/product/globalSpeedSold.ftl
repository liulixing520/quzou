define('mm-ae-cate-bestselling-pitems', function(require, exports) {

	// var anchor = require('js/5v/run/intl-activities/app/anchor/anchor.js');

	// ģʼ
	exports.init = function(box, module) {
		// 
		var LazyloadImages = require("");
		var mustache = require('');
		// var cookieInfo = require("js/6v/biz/common/cookie-info/cookie-info.js");
		var Flipsnap = require("");
		var AsyncData = require('')

		// ò
		var fsPageItems = 10; // ÿҳƷĬ10
		var letterNums = 55;
		// common
		var template = $(".X-template-bs-products").html();
		var scroller = $($.browser.webkit ? 'body' : 'html');
		var $win = $(window);
		var scrollbarWidth = getScrollbarWidth() || 17;
		var optionsCache = {};
		var hasFixedClass = false;
		var fixedHeight;

		// 
		var ttids = [];
		// var memberSeq = cookieInfo.getIsLoggedIn() ? cookieInfo.getMemberSeq() : "";
		// var ajaxUrl = "http://us.ladder.aliexpress.com/.dox?action=ajax/LadderViewDataAction&event_submit_doQueryData=true&offset=1&limit=60&isPushData=y&memberSeq=" + memberSeq + "&widgetId=";

		// Բ
		var orderMulLang = (window.I18N && window.I18N['othersMap'] && window.I18N['othersMap']['orders']) ? window.I18N['othersMap']['orders'] : "Orders";
		var feedbackMulLang = (window.I18N && window.I18N['othersMap'] && window.I18N['othersMap']['feedback']) ? window.I18N['othersMap']['feedback'] : "Feedback";

		// flipsnap
		var fsPerRowItems = $(".mm-ae-cate-bestselling-pitems").outerWidth(true) + scrollbarWidth > 1019 ? 5 : 4;
		var fsPages = []; // ÿflipsnap ķҳ
		var fsIdPrefix = "flipsnap-";
		var flipsnapSet = []; // flipsnap 
		var fsRepeat = "repeat";

		var winSafariReg = /windows.* \bversion\/([0-9.]+(?: beta)?).*safari\//i;
		var isWinSafari = winSafariReg.test(navigator.userAgent);

		//  Ŀеĵ¼
		$(".mm-ae-cate-bestselling-pitems .cates-nav").on("click", function(e) {
			$(this).children().removeClass("active");
			$(e.target).hasClass("tab") ? $(e.target).addClass("active") : $(e.target).parent(".tab").addClass("active");
		});
		// ȡҳttid
		ttids = getAllTtids(".mm-ae-cate-bestselling-pitems .flipsnap");
		setTimeout(function() {
			AsyncData.loadData({
				dataType: "jsonp",
				type: "tt",
				info: ttids,
				offset: 1,
				limit: 60,
				isPushData: "n",
				newInterFace: true
			}).done(function(data) {
				$(data).each(function(i, v) {
					var nodeId = v.nodeTree[0].nodeId || '';
					setTimeout(function() {
						AsyncData.loadData({
							dataType: "jsonp",
							type: "tt",
							info: [ttids[i]],
							nodeId: nodeId,
							offset: 1,
							limit: 60,
							isPushData: "y",
							newInterFace: true
						}).done(function(data) {
							data = data[0];
							if (data && data.productList) {
								$("#bestselling-" + i + ttids[i] + " .products-pane .loading").hide();
								var productList = [];
								var product;
								var fsWrapSelector = "#" + fsIdPrefix + i + ttids[i];

								var pageNums = 0;
								var fsEle = $(fsWrapSelector + " .viewport .flipsnap");
								data.productList = data.productList.length > 0 ? data.productList.slice(0, 40) : [];
								productList = processListData(data.productList, {
									orderMulLang: orderMulLang,
									feedbackMulLang: feedbackMulLang
								});
								pageNums = Math.floor(productList.length / fsPageItems);
								if (productList.length % fsPageItems !== 0) {
									pageNums++;
								}

								fsPages.push(pageNums);
								flipsnapSet.push(fsEle);

								renderFlipSnap({
									fsEle: fsEle,
									tpl: template,
									productList: productList,
									pages: pageNums,
									fsPageItems: fsPageItems,
									fsPerRowItems: fsPerRowItems
								});
								// Ϊÿviewport ̶߶
								$(fsWrapSelector + " .viewport").height($(fsWrapSelector + " .viewport .product-item").outerHeight(true) * 2 + 1);

								$(fsWrapSelector + " .bs-slide-prev-btn").show();
								$(fsWrapSelector + " .bs-slide-next-btn").show();


								if (!isWinSafari) {
									new LazyloadImages({
										element: fsWrapSelector + ' .flipsnap img[data-src]',
										srcAttribute: 'data-src',
										oneoff:true
									});
								} else {
									$(fsWrapSelector + ' .flipsnap img[data-src]').each(function() {
										$(this).attr('src', $(this).attr('data-src')).removeAttr('data-src');
									});
								}


								var flipsnap = new Flipsnap(fsWrapSelector + " .flipsnap", {
									slide: fsRepeat,
									responsive: true,
									transitionDuration: 350
								});


								$(fsWrapSelector + ' .bs-slide-prev-btn').on('click', function(ev) {
									flipsnap.toPrev();
									var index = flipsnap.currentPoint;
									if (index > pageNums) {
										index = 1;
									} else if (index === 0) {
										index = pageNums;
									}
									$(fsWrapSelector + ' .flipsnap').find(".flipsnap-item").eq(index).find("img[data-src]").each(function(index, val) {
										$(this).attr('src', $(this).attr('data-src')).css('visibility', 'visible').removeAttr('data-src');
									});

								});
								$(fsWrapSelector + ' .bs-slide-next-btn').on('click', function(ev) {
									flipsnap.toNext();
									if (!isWinSafari) {
										var index = flipsnap.currentPoint;
										if (index > pageNums) {
											index = 1;
										} else if (index === 0) {
											index = pageNums;
										}
										$(fsWrapSelector + ' .flipsnap').find(".flipsnap-item").eq(index).find("img[data-src]").each(function(index, val) {
											$(this).attr('src', $(this).attr('data-src')).css('visibility', 'visible').removeAttr('data-src');
										});
									}
								});
								$(fsWrapSelector + ' .flipsnap').on('fsmoveend', function(e) {
									if (!isWinSafari ) {
										var index = flipsnap.currentPoint;
										if (index > pageNums) {
											index = 1;
										} else if (index === 0) {
											index = pageNums;
										}
										$(this).find(".flipsnap-item").eq(index).find("img[data-src]").each(function(index, val) {
											$(this).attr('src', $(this).attr('data-src')).css('visibility', 'visible').removeAttr('data-src');
										});
									}
								});
							}

						});
					}, 100)
				});
			});
		}, 0);

		$(window).on("resize", function(e) {
			var curFsPerRowItems = $(".mm-ae-cate-bestselling-pitems").outerWidth(true) + scrollbarWidth > 1019 ? 5 : 4;
			if (curFsPerRowItems === fsPerRowItems) {
				return;
			}
			// rerank all products except the first  flipsnap item
			for (var i = 0, len = fsPages.length; i < len; i++) {
				rerankProducts({
					flipsnap: flipsnapSet[i],
					repeat: fsRepeat,
					fsPerRowItems: curFsPerRowItems,
					fsPages: fsPages[i]
				});
			}
			fsPerRowItems = curFsPerRowItems;
		});
		$(window).on("scroll", function(e) {
			if (!hasFixedClass) {
				fixedHeight = $("#bs-fixed-nav").offset().top;
			}
			var scrollTop = $(window).scrollTop();
			if (scrollTop > fixedHeight && !hasFixedClass) {
				$("#bs-fixed-nav").addClass("fixed");
				hasFixedClass = true;
			} else if (scrollTop < fixedHeight && hasFixedClass) {
				$("#bs-fixed-nav").removeClass("fixed");
				hasFixedClass = false;
			}
		});
		$win.on('scroll', debounce(300, function(e) {
			for (var key in optionsCache) {
				_anchorSlideTo(optionsCache[key], $win.scrollTop());
			}
		}.bind(this), true));

		function getAllTtids(selector) {
			var ttids = [];
			$(selector).each(function(index) {
				ttids.push($(this).attr("data-ttid"));
			});
			return ttids;
		}

		function renderFlipSnap(obj) {
			var $flipsnap = $(obj.fsEle);
			var productList = obj.productList;

			for (var i = 0; i < obj.pages; i++) {
				var products = productList.slice(i * obj.fsPageItems, (i + 1) * obj.fsPageItems);
				if (obj.fsPerRowItems == 4) {
					$(products).each(function(index, val) {
						products[index]['rank'] -= i * 2;
					});
				}
				$flipsnap.append(mustache.render(obj.tpl, {
					productList: products
				}));
			}
		}
		// Ʒ
		function rerankProducts(obj) {
			var fsPages = obj.fsPages;
			var showNums = obj.fsPerRowItems * 2;

			var $fsItems = $(obj.flipsnap).find(".flipsnap-item");

			if ($fsItems.length > 0) {
				if (obj.repeat == "repeat") {
					$fsItems = $fsItems.slice(2, fsPages + 1);
				} else {
					$fsItems = $fsItems.slice(1);
				}
			}
			$fsItems.each(function(index) {
				$(this).find(".rank").each(function(i) {
					var newRank = showNums * (index + 1) + i + 1;
					$(this).text("Top " + newRank);
				});
			});
		}

		function processListData(list, options) {
			var orderMulLang = options.orderMulLang;
			var feedbackMulLang = options.feedbackMulLang;
			var productList = [];
			var product;
			$.each(list || [], function(i, val) {
				var feedback = feedbackMulLang + '(' + val.productFeedbackNum + ')';

				if (feedback.length > 12) {
					feedback = feedback.substring(0, 9) + "...";
				}
				product = {
					rank: i + 1,
					productOrderNum: val.productOrderNum + orderMulLang,
					productDetailUrl: val.productDetailUrl,
					productImgUrl: val.productImgUrl,
					productName: val.productName && val.productName.length > letterNums ? val.productName.substring(0, letterNums - 3) + "..." : val.productName,
					productMinPrice: val.productMinPrice,
					productUnitType: (window.I18N && window.I18N.unitMap && window.I18N.unitMap[(val.productUnitType+"").toLowerCase()]) ? window.I18N.unitMap[val.productUnitType.toLowerCase()] : val.productUnitType,
					rate: val.productStarRating + "%",
					feedback: feedback
				};
				productList.push(product);
			});
			return productList;
		}

		function getScrollbarWidth() {
			var noScroll, scroll, oDiv = document.createElement("DIV");
			oDiv.style.cssText = "position:absolute; top:-1000px; width:100px; height:100px; overflow:hidden;";
			noScroll = document.body.appendChild(oDiv).clientWidth;
			oDiv.style.overflowY = "scroll";
			scroll = oDiv.clientWidth;
			document.body.removeChild(oDiv);
			return noScroll - scroll;
		}

		function throttle(delay, action, tail, debounce) {
			var now = Date.now,
				last_call = 0,
				last_exec = 0,
				timer = null,
				curr, diff,
				ctx, args, exec = function() {
					last_exec = now();
					action.apply(ctx, args);
				};

			return function() {
				ctx = this, args = arguments,
				curr = now(), diff = curr - (debounce ? last_call : last_exec) - delay;

				clearTimeout(timer);

				if (debounce) {
					if (tail) {
						timer = setTimeout(exec, delay);
					} else if (diff >= 0) {
						exec();
					}
				} else {
					if (diff >= 0) {
						exec();
					} else if (tail) {
						timer = setTimeout(exec, -diff);
					}
				}

				last_call = curr;
			};
		};

		function debounce(idle, action, tail) {
			return throttle(idle, action, tail, true);
		};

		function _getNearestIndex(options, scrollTop) {
			var curIndex = 0,
				nearestOffset = null;
			options.elements.each(function(i) {
				var target = $(this.hash);
				if (target.length) {
					var targetOffsetTop = target.offset().top + options.offsetY,
						offset = Math.abs(scrollTop - targetOffsetTop);
					if (targetOffsetTop <= scrollTop) {
						if (nearestOffset == null) {
							nearestOffset = offset;
							curIndex = i;
						} else if (offset < nearestOffset) {
							nearestOffset = offset;
							curIndex = i;
						}
					}
				}
			});
			return curIndex;
		}

		function _anchorSlideTo(options, scrollTop) {
			var curIndex = _getNearestIndex({
				elements: options.elements,
				offsetY: options.offsetY
			}, scrollTop);

			options.classElements.removeClass(options.curentClass).eq(curIndex).addClass(options.curentClass);
			if (typeof options.addedCurrentCallback == 'function') {
				options.addedCurrentCallback(options.classElements.eq(curIndex));
			}
		}
		$.fn.anchor = function(options) {
			options.offsetY = Number(options.offsetY) || 0;
			options.curentClass = options.curentClass || 'current';
			options.elements = options.elements ? $(options.elements) : this;
			options.classElements = options.classElements ? $(options.classElements) : this;

			if (this.data('_anchor_binded_') || this.length == 0) {
				return this;
			}
			this.data('_anchor_binded_', 'true');
			optionsCache[this[0][$.expando]] = options;
			this.on("click", function(e) {
				e.preventDefault();
				var target = $(this.hash);
				scroller.animate({
					/* +1 : fix firefox anchor   */
					scrollTop: target.offset().top + options.offsetY + 1
				}, options.animateDuring);
			});
			_anchorSlideTo(options, $win.scrollTop());
		};

		$('#bs-cates-nav .tab a').anchor({
			offsetY: -90,
			curentClass: "active",
			elements: "#bs-cates-nav .tab a",
			classElements: "#bs-cates-nav .tab",
			animateDuring: 100
		});
	};

});
