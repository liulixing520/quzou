	<script>
	    function alertCancel(message) {
	        if ($('#_mask')) {
	            $('#_mask').addClass("pop-layer");
	        }
	        $("#alertCancelText").text(message);
	        $("#alertCancel").show();
	    }
	    function hideAlert() {
	        if ($('#_mask')) {
	            $('#_mask').removeClass("pop-layer");
	        }
	        $(".pop-box").hide();
	    }
	    function submitCancel() {
	        var serviceId = jQuery('#serviceId').val();
	        jQuery.get(
	                '/afs/cancelApply.json?sid=046753ecacf07bb42a3ec5377f31bb2a',
	                {"serviceId": serviceId},
	                function (data) {
	                    hideAlert();
	                    if (data.flag == true) {
	                        alert(data.message);
	                        location.reload();
	                    } else {
	                        alert(data.message);
	                        location.reload(true);
	                    }
	                }, "json");
	    }
	
	
	    //取消进度
	    function oncancelserviceID(serviceId) {
	        if (0 == 1
	    )
	        {
	            window.external.Notify("confirm:确定要取消本次售后申请吗？" + serviceId);
	        }
	    else
	        {
	            jQuery('#serviceId').val(serviceId);
	            alertCancel('确定要取消本次售后申请吗？');
	                                                                                                                                                        }
	    }
	    function ok(serviceId) {
	        jQuery.get('/afs/handelSolution.json?sid=046753ecacf07bb42a3ec5377f31bb2a', {"serviceId": serviceId, "pageSize":1},
	                function (data) {
	                    if (0 == 0
	                    )
	                    {
	                        if (data.optState == 1) {
	                            alert("您的售后服务已完成,感谢您对京东的信任!\n欢迎继续购物!");
	                            location.reload(true);
	                        }
	                        else {
	                            alert(data.message);
	                        }
	                    }
	                    else
	                    {
	                        if (data.optState == 1) {
	                            window.external.Notify("msg:您的售后服务已完成,感谢您对京东的信任!\n欢迎继续购物!");
	                            window.location.href = '/afs/repairList?page=1&sid=046753ecacf07bb42a3ec5377f31bb2a';
	                        } else {
	                            window.external.Notify("msg:" + data.message);
	                        }
	                    }
	                }, "json");
	    }
	
	
	    $('#repair_page').val(1);
	
