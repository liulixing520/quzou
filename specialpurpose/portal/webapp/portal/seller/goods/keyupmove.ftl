 /**
  * 验证是不是还可以上传新的图片
  * */
 function validateOnSelect(event, ID, fileObj,callBackObj){
 	if(fileObj.size >   1024 * 1024){
		
		$("#operate_message").html("您上传文件的大小不能超过1M。");
		$("#operate_message").show();
	
 		return false;
 	}	
 	if((fileObj.type).toLowerCase() != ".jpg" 
 		&& (fileObj.type).toLowerCase() != ".png"){
		
		$("#operate_message").html("您上传的文件格式必须是jpg/png格式。");
		$("#operate_message").show();
    		
 		return false;
 	}
	$("#operate_message").hide();
 	return true;
 }
