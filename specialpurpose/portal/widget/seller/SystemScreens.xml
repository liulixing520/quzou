	<screen name="EditDistributionRegion">
		<section>
			<actions>
				<set field="titleProperty" value="" />
				<set field="currentForm" value="EditDistributionRegion" />
				<set field="geoId" from-field="parameters.geoId" />
				<entity-one value-field="geo" entity-name="Geo"></entity-one>
				<entity-one value-field="geoDistribution" entity-name="GeoDistribution"></entity-one>
				<entity-condition list="geoDistributionUsers" entity-name="GeoDistributionUser">
					<condition-list>
						<condition-expr field-name="geoId" from-field="geoId" />
