	<screen name="FindDistributionRegion">
		<section>
			<actions>
				<property-map resource="AdminUiLabels" map-name="uiLabelMap" global="true" />
				<set field="parameters.noConditionFind" value="Y" />
				<set field="parameters.geoTypeId" value="GROUP" />
				<set field="parameters.sortField" from-field="parameters.sortField" default-value="sequenceNum"/>
				<set field="searchAction" value="FindDistributionRegion" />
				<set field="searchForm" value="FindDistributionRegion" />
			</actions>
			<widgets>
				<decorator-screen name="product-decorator">
					<decorator-section name="body">
						<decorator-screen name="FindScreenDecorator" location="${parameters.mainDecoratorLocation}">
