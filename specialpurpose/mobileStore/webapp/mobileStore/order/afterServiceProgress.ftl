	    function viewBusinessAddress(serviceId) { //在中间弹出层显示商家地址
	        jQuery.get('/afs/getAddressByServiceId.json?sid=046753ecacf07bb42a3ec5377f31bb2a', {"serviceId": serviceId},
	                function (data) {
	                    var joinStr = "";
	                    $("#viewDivContentId").empty();
	                    $(".bounceDiv .btn-close")
	                            .attr("style", "top:0px;");
	                    if (data != null && data.bizdictValueAddress != null
	                            ) {
	                        var num = 0;
	                        if (data.bizdictValueAddress.address != null
	                                && data.bizdictValueAddress.address
	                                .trim() != "") {
	                            joinStr = "<li>商家地址："
	                                    + data.bizdictValueAddress.address
	                                    + "</li>";
	                        } else {
	                            joinStr = "<li>商家地址：暂无</li>";
	                            num++;
	                        }
	                        if (data.bizdictValueAddress.linkMan != null
	                                && data.bizdictValueAddress.linkMan
	                                .trim() != "") {
	                            joinStr += "<li>联系人："
	                                    + data.bizdictValueAddress.linkMan
	                                    + "</li>";
	                        } else {
	                            joinStr += "<li>联系人：暂无</li>";
	                            num++;
	                        }
	                        if (data.bizdictValueAddress.tel != null
	                                && data.bizdictValueAddress.tel.trim() != "") {
	                            joinStr += "<li>联系电话："
	                                    + data.bizdictValueAddress.tel
	                                    + "</li>";
	                        } else {
	                            joinStr += "<li>联系电话：暂无</li>";
	                            num++;
	                        }
	                        if (num == 3) {
	                            joinStr = "<li>系统忙请稍后再试，您也可以到网站查看邮寄地址。</li>";
	                        }
	                    } else {
	                        joinStr = "<li>系统忙请稍后再试，您也可以到网站查看邮寄地址。</li>";
	                    }
	                    if (joinStr.indexOf("系统忙请稍后再试") != -1) {
	                        $(".bounceDiv .btn-close").attr("style",
	                                "top:55px;");
	                    }
	                    $("#viewDivContentId").append(joinStr);
	                    $("#fade").show();
	                    $("#light").show();
	                }, "json");
	    }
	</script>
	
